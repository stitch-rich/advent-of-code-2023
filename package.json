{
  "name": "advent-of-code",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest --verbose",
    "test:cover": "jest --verbose --coverage",
    "test:watch": "jest --verbose --watch --notify",
    "lint": "eslint './src/**/*'"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "clearMocks": true,
    "testPathIgnorePatterns": [
      "node_modules",
      "build"
    ],
    "coverageReporters": [
      "html",
      "text",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.191",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.3.1",
    "node-notifier": "^10.0.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
